<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.collections.*?>

<BorderPane fx:id="root" id="borderpane" prefHeight="900.0" prefWidth="900.0" styleClass="mainFxmlClass" 
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="bank.controller.CustomerController">
    <padding>
        <Insets top="15" right="12" left="15" bottom="12"/>
    </padding>
    
    <top>
        <HBox spacing="10">
            <TabPane fx:id="tabPane" maxWidth="-Infinity">
                <tabs>
                    <Tab text="New Account" closable="false">
                        <fx:include source="NewAccount.fxml" />
                    </Tab>
                </tabs>
            </TabPane>
        </HBox>
    </top>
    
    
    
    <stylesheets>
        <URL value="@bank/views/graphics/customerview.css"/>
    </stylesheets>
</BorderPane>
